// map_int_int_test.eo - Map[int]int 类型支持测试

func main() -> int {
    print "=== Testing map[int]int ==="
    
    // 创建一个空map[int]int
    let counts: map[int]int = {}
    
    // 测试设置键值对
    counts[1] = 10
    counts[2] = 20
    counts[3] = 30
    
    print "Map initialized with 3 entries"
    
    // 测试获取值
    let count1: int = counts[1]
    print count1  // 应该输出 10
    
    let count2: int = counts[2]
    print count2  // 应该输出 20
    
    let count3: int = counts[3]
    print count3  // 应该输出 30
    
    // 测试更新值
    counts[1] = 15
    let updatedCount: int = counts[1]
    print updatedCount  // 应该输出 15
    
    // 测试删除键值对
    delete(counts, 2)
    print "Deleted key 2"
    
    print "=== Map[int]int test completed ==="
    return 0
}
