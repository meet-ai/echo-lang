// trait_associated_types_working_test.eo - 工作的关联类型测试
trait Container[T] {
    type Item;  // 关联类型声明

    func get() -> Item
    func size() -> int
}

@impl Container[int]

type Item = int;  // 关联类型实现

struct IntArray {
    data: [int]
}

func (ia IntArray) get() -> int {
    return ia.data[0]  // 简化实现
}

func (ia IntArray) size() -> int {
    return len(ia.data)
}

func main() -> int {
    let arr: IntArray = {data: [1, 2, 3, 4, 5]}
    print arr.size()
    print arr.get()
    return 0
}
