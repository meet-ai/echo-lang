// 简单的类型转换运行时测试
// 只测试 string.split() 的类型转换，不调用 len()

import string

func main() -> int {
    print "=== 类型转换功能测试 ==="
    
    // 测试 string.split() 使用新的类型转换
    let s: string = "hello,world,test"
    print "原始字符串: " + s
    
    // 调用 split，这会使用 []string(result) 类型转换
    let parts: []string = s.split(",")
    print "分割完成"
    
    print "=== 测试完成 ==="
    return 0
}
