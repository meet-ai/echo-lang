// async_await_test.eo - 简单的async测试
async func getMessage() -> string {
    print "Async function called"
    return "Hello async!"
}

func main() -> int {
    print "=== Simple Async Test ==="
    let message: string = await getMessage()
    print "Received: " + message
    print "=== Test Completed ==="
    return 0
}
