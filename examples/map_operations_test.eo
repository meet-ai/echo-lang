// map_operations_test.eo - Map操作功能测试（len, clear, contains）

func main() -> int {
    print "=== Testing Map Operations ==="
    
    // 测试 map[string]string
    let m1: map[string]string = {}
    m1["a"] = "apple"
    m1["b"] = "banana"
    m1["c"] = "cherry"
    
    // 测试 len()
    let len1: int = len(m1)
    print len1  // 应该输出 3
    
    // 测试 contains()
    let hasA: int = m1.contains("a")
    print hasA  // 应该输出 1（true）
    
    let hasD: int = m1.contains("d")
    print hasD  // 应该输出 0（false）
    
    // 测试 clear()
    m1.clear()
    let len2: int = len(m1)
    print len2  // 应该输出 0
    
    // 测试 map[int]string
    let m2: map[int]string = {}
    m2[1] = "one"
    m2[2] = "two"
    m2[3] = "three"
    
    let len3: int = len(m2)
    print len3  // 应该输出 3
    
    let has1: int = m2.contains(1)
    print has1  // 应该输出 1
    
    let has4: int = m2.contains(4)
    print has4  // 应该输出 0
    
    m2.clear()
    let len4: int = len(m2)
    print len4  // 应该输出 0
    
    print "=== Map Operations test completed ==="
    return 0
}
