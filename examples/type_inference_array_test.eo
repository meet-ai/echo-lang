// 测试数组字面量类型推断
func main() -> int {
    // 测试整数数组推断
    let a = [1, 2, 3]           // 推断为 [int]
    let b = [10, 20, 30, 40]    // 推断为 [int]
    
    // 测试浮点数数组推断
    let c = [1.0, 2.0, 3.0]     // 推断为 [float]
    let d = [3.14, 2.71]        // 推断为 [float]
    
    // 测试字符串数组推断
    let e = ["a", "b", "c"]     // 推断为 [string]
    let f = ["hello", "world"]  // 推断为 [string]
    
    // 测试布尔数组推断
    let g = [true, false]       // 推断为 [bool]
    
    print "Array type inference test passed"
    return 0
}
