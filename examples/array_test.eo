// array_test.eo - 数组功能测试
let numbers: [int] = [1, 2, 3, 4, 5]
print 42

// 测试长度获取（先测试这个，因为它不依赖索引）
let length: int = len(numbers)
print length

// 测试索引访问
let first: int = numbers[0]
print first

let second: int = numbers[1]
print second

// 测试切片操作
let slice_start: [int] = numbers[1:3]
print "Slice created"

let slice_end: [int] = numbers[:3]
print "Slice end created"

let slice_all: [int] = numbers[:]
print "Slice all created"
