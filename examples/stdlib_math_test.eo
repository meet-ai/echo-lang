// 标准库 Math 模块集成测试
// 验证数学函数是否正确调用运行时函数

import math

func main() -> int {
    print_string("=== Math Module Integration Test ===\n")
    
    // 测试 sqrt() 函数 - 调用 runtime_math_sqrt()
    let sqrt_result = math.sqrt(16.0)
    print_string("sqrt(16.0) called successfully\n")
    
    // 测试 pow() 函数 - 调用 runtime_math_pow()
    let pow_result = math.pow(2.0, 3.0)
    print_string("pow(2.0, 3.0) called successfully\n")
    
    // 测试 sin() 函数 - 调用 runtime_math_sin()
    let sin_result = math.sin(0.0)
    print_string("sin(0.0) called successfully\n")
    
    // 测试 cos() 函数 - 调用 runtime_math_cos()
    let cos_result = math.cos(0.0)
    print_string("cos(0.0) called successfully\n")
    
    // 测试 exp() 函数 - 调用 runtime_math_exp()
    let exp_result = math.exp(1.0)
    print_string("exp(1.0) called successfully\n")
    
    // 测试 ln() 函数 - 调用 runtime_math_ln()
    let ln_result = math.ln(1.0)
    print_string("ln(1.0) called successfully\n")
    
    print_string("Math module test completed successfully\n")
    return 0
}
