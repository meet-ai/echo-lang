// 测试切片表达式类型推断
// 注意：切片表达式需要从数组变量推断，需要符号表支持（第二阶段）
// 当前阶段：先测试数组字面量推断，切片表达式推断需要符号表支持
func main() -> int {
    // 创建数组（带显式类型，因为变量引用需要符号表支持）
    let arr: [int] = [1, 2, 3, 4, 5]
    let strArr: [string] = ["a", "b", "c", "d"]
    
    // 切片表达式类型推断需要符号表支持（第二阶段实现）
    // 当前阶段：切片表达式需要显式类型注解
    let slice1: [int] = arr[1:3]
    let slice2: [int] = arr[:3]
    let slice3: [int] = arr[1:]
    let slice4: [string] = strArr[0:2]
    let slice5: [string] = strArr[:2]
    let slice6: [string] = strArr[2:]
    
    print "Slice expression test (explicit types for now)"
    return 0
}
