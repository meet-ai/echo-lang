// comparison_test.eo - 比较运算测试
func main() -> int {
    let a: int = 10
    let b: int = 20

    // 直接使用比较结果
    if a < b {
        print "a < b is true"
    }

    if a > b {
        print "a > b is true"
    } else {
        print "a > b is false"
    }

    if a == a {
        print "a == a is true"
    }

    return 0
}
