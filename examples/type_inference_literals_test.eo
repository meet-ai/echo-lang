// type_inference_literals_test.eo - 测试 Ok/Err/Some/None 字面量类型推断

func main() -> int {
    // 测试1：Ok 字面量类型推断
    let result1 = Ok(42)  // 应该推断为 Result[int, string]
    print "Ok literal test 1"

    // 测试2：Ok 字面量从变量推断
    let value: int = 100
    let result2 = Ok(value)  // 应该推断为 Result[int, string]
    print "Ok literal test 2"

    // 测试3：Some 字面量类型推断
    let opt1 = Some(42)  // 应该推断为 Option[int]
    print "Some literal test 1"

    // 测试4：Some 字面量从变量推断
    let str: string = "hello"
    let opt2 = Some(str)  // 应该推断为 Option[string]
    print "Some literal test 2"

    // 测试5：Err 字面量（需要显式类型注解）
    // let result3 = Err("error")  // 需要显式类型：let result3: Result[int, string] = Err("error")
    let result3: Result[int, string] = Err("error")
    print "Err literal test (with explicit type)"

    // 测试6：None 字面量（需要显式类型注解）
    // let opt3 = None  // 需要显式类型：let opt3: Option[int] = None
    let opt3: Option[int] = None
    print "None literal test (with explicit type)"

    print "Ok/Err/Some/None literals type inference test passed"
    return 0
}
